package body Datum is
   procedure Get(K : Datum_Type) is 
      Text : String(1..4);
      Char : Character;
   begin
      Get(Text(1..4));
      K.Y := Integer'Value(Text(1..4));
      Get(Char);
      Get(Text(1..2));
      K.Y := Integer'Value(Text(1..2));
      Get(Char);
      Get(Text(1..2));
      K.Y := Integer'Value(Text(1..2));
      Get(Char);
      
	  
      
  end Get;
   
   
end Datum;
