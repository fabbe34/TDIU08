#include <iostream>
#include <cmath>

using namespace std;
int N(int n_fak);
void Swap(int & left,int & right);
int main()
{
  int summa{ N(10)};
  cout<< summa << endl;
  int a {5};
  int b {10};
  Swap(a,b);
  cout << a << b<< endl;

}
int N(int n_fak)
{
  int summa{1};
  for(int i =1; i<n_fak+1;i++)
    {
      summa *= i;
    }
  return summa;
}
void Swap(int & left,int & right)
{
  int b = left;
  left= right;
  right = right;

}
